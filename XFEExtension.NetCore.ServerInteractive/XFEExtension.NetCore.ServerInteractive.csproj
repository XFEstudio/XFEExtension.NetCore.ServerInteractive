<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFramework>net8.0</TargetFramework>
		<ImplicitUsings>enable</ImplicitUsings>
		<Nullable>enable</Nullable>
		<GenerateDocumentationFile>True</GenerateDocumentationFile>
		<Version>1.0.0</Version>
		<Title>XFEExtension.NetCore.ServerInteractive</Title>
		<RepositoryUrl>https://github.com/XFEstudio/XFEExtension.NetCore.ServerInteractive</RepositoryUrl>
		<AnalysisLevel>latest</AnalysisLevel>
		<PackAsTool>False</PackAsTool>
		<GeneratePackageOnBuild>True</GeneratePackageOnBuild>
		<SignAssembly>True</SignAssembly>
		<Copyright>寰宇朽力网络科技有限公司版权所有</Copyright>
		<PackageProjectUrl>https://github.com/XFEstudio/XFEExtension</PackageProjectUrl>
		<PackageIcon>logoIcon.png</PackageIcon>
		<Description>Server interaction extension, including user identity verification and querying in conjunction with AutoConfig</Description>
		<PackageReadmeFile>README.md</PackageReadmeFile>
		<PackageTags>XFE;Server;服务器;XFEExtension</PackageTags>
		<PackageReleaseNotes>
			## 调整

			无

			## 新增

			无

			## 严重

			无
		</PackageReleaseNotes>
		<NeutralLanguage>zh-CN</NeutralLanguage>
		<PackageRequireLicenseAcceptance>True</PackageRequireLicenseAcceptance>
		<PackageLicenseFile>LICENSE</PackageLicenseFile>
		<AssemblyOriginatorKeyFile>..\XFEstudio.pfx</AssemblyOriginatorKeyFile>
		<Version>$(Version)</Version>
		<Authors>XFEstudio</Authors>
	</PropertyGroup>

	<ItemGroup>
		<PackageReference Include="System.Management" Version="9.0.9" />
		<PackageReference Include="XFEExtension.NetCore" Version="3.3.6" />
		<PackageReference Include="XFEExtension.NetCore.AutoConfig" Version="2.0.*" />
		<PackageReference Include="XFEExtension.NetCore.AutoImplement" Version="1.1.*" />
	</ItemGroup>

	<ItemGroup>
	  <Folder Include="Utilities\Services\" />
	</ItemGroup>

</Project>
